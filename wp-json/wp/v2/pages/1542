red = isMinor;\n                showError(input.id, !input.value && isMinor);\n            });\n            validateField(e.target);\n        });\n\n        document.getElementById('powderhorn').addEventListener('change', (e) => {\n            const powderhorn = e.target.value === 'yes';\n            const coloradoClassic = document.getElementById('coloradoClassic').value === 'yes';\n            const isEvent = powderhorn || coloradoClassic;\n            document.querySelectorAll('.powderhorn-field').forEach(field => {\n                field.classList.toggle('hidden', !powderhorn);\n                const input = field.querySelector('input, select');\n                input.required = powderhorn;\n                showError(input.id, !input.value && powderhorn);\n            });\n            document.querySelectorAll('.event-field').forEach(field => {\n                field.classList.toggle('hidden', !isEvent);\n                const input = field.querySelector('input, select');\n                input.required = isEvent;\n                showError(input.id, !input.value && isEvent);\n            });\n            showError('powderhorn', !e.target.value);\n        });\n\n        document.getElementById('coloradoClassic').addEventListener('change', (e) => {\n            const coloradoClassic = e.target.value === 'yes';\n            const powderhorn = document.getElementById('powderhorn').value === 'yes';\n            const isEvent = powderhorn || coloradoClassic;\n            document.querySelectorAll('.colorado-classic-field').forEach(field => {\n                field.classList.toggle('hidden', !coloradoClassic);\n                const input = field.querySelector('input, select');\n                if (input.id !== 'secondTeam') {\n                    input.required = coloradoClassic;\n                    showError(input.id, !input.value && coloradoClassic);\n                }\n            });\n            document.querySelectorAll('.event-field').forEach(field => {\n    