\n                isValid = false;\n                message = 'Please enter a valid date (MM\/DD\/YYYY).';\n            } else if (id === 'state' && !\/^[A-Z]{2}$\/.test(value)) {\n                isValid = false;\n                message = 'Please enter a valid two-letter state code.';\n            } else if (id === 'zip' && !\/^\\d{5}$\/.test(value)) {\n                isValid = false;\n                message = 'Please enter a valid 5-digit ZIP code.';\n            } else if (field.type === 'number' && (value < 0 || !Number.isInteger(Number(value)))) {\n                isValid = false;\n                message = `Please enter a valid ${field.name}.`;\n            }\n\n            showError(id, !isValid && !onSubmit, message);\n            return isValid;\n        }\n\nasync function fetchGroups() {\n    try {\n        document.getElementById('groupCodeSpinner').style.display = 'block';\n        document.getElementById('groupCodeTSpinner').style.display = 'block';\n        const response = await fetch('https:\/\/script.google.com\/macros\/s\/AKfycbxQT1mhqEf3-RAB75o4ct27Jp7so5fs-JIdNaSykBtRD3MVY2OycLPJqO_W5pZKOSLh1Q\/exec?sheet=WEB: Groups');\n        const data = await response.json();\n        \/\/ Sort data by group code (row[2]) alphabetically\n        data.sort((a, b) => (a[2] || '').localeCompare(b[2] || ''));\n        const groupSelects = [document.getElementById('groupCode'), document.getElementById('groupCode_t')];\n        groupSelects.forEach(select => {\n            select.innerHTML = '<option value=\"\">Select Group';\n            data.forEach(row => {\n                if (row[2]) {\n                    const option = document.createElement('option');\n                    option.value = row[2];\n                    option.textContent = `${row[2]} - ${row[3]} (${row[4]})`;\n                    select.appendChild(option);\n                }\n            });\n        });\n    } catch (error) {\n        console.error('Error fetching groups:', error);\n    