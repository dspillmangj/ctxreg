0].slice(0, 3).padEnd(3, 'X');\n            }\n            let thirdChar = isTeam ? 'C' : abbr[0] === 'C' ? 'X' : abbr[0];\n            let code = `26${thirdChar}${abbr.slice(1, 3)}`;\n            let attempt = 0;\n            const maxAttempts = 100;\n\n            while (attempt < maxAttempts) {\n                try {\n                    const response = await fetch(`https:\/\/script.google.com\/macros\/s\/AKfycbxQT1mhqEf3-RAB75o4ct27Jp7so5fs-JIdNaSykBtRD3MVY2OycLPJqO_W5pZKOSLh1Q\/exec?sheet=${isTeam ? 'WEB: Teams' : 'WEB: Groups'}&${isTeam ? 'teamCode' : 'groupCode'}=${code}`);\n                    const data = await response.json();\n                    if (data.length === 0) {\n                        return code;\n                    }\n                    attempt++;\n                    const suffix = String.fromCharCode(65 + (attempt % 26)) + String.fromCharCode(65 + (Math.floor(attempt \/ 26) % 26));\n                    code = `26${thirdChar}${suffix}`;\n                } catch (error) {\n                    console.error('Error checking code uniqueness:', error);\n                    return null;\n                }\n            }\n            throw new Error('Unable to generate a unique code after maximum attempts');\n        }\n\n        function getTimestamps() {\n            const now = new Date();\n            return {\n                iso: now.toISOString(),\n                mst: now.toLocaleString('en-US', { timeZone: 'America\/Denver' })\n            };\n        }\n\n        async function submitIndividualForm(event) {\n            event.preventDefault();\n            const submitButton = document.getElementById('individualSubmit');\n            submitButton.disabled = true;\n            submitButton.classList.add('loading');\n            const form = event.target;\n            const formData = new FormData(form);\n            const age = parseInt(formData.get('age') || 0, 10);\n            const isMinor = age < 18;\n            const powderhorn =