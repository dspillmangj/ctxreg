    <script>
        function openTab(tabName) {
            document.querySelectorAll('.tab-content').forEach(tab => tab.classList.remove('active'));
            document.querySelectorAll('.tab-button').forEach(btn => btn.classList.remove('active'));
            document.getElementById(tabName).classList.add('active');
            document.querySelector(`button[onclick="openTab('${tabName}')"]`).classList.add('active');
        }

        function showError(fieldId, show, message = '') {
            const errorElement = document.getElementById(`${fieldId}Error`);
            if (errorElement) {
                errorElement.style.display = show ? 'block' : 'none';
                if (message) errorElement.textContent = message;
            }
        }

        function validateField(field, onSubmit = false) {
            const id = field.id;
            const value = field.value;
            let isValid = true;
            let message = '';

            if (field.required && !value) {
                isValid = false;
                message = onSubmit ? `Please enter a value for ${field.name}.` : '';
            } else if (field.type === 'tel' && field.pattern && !new RegExp(field.pattern).test(value)) {
                isValid = false;
                message = 'Please enter a valid 10-digit phone number.';
            } else if (field.type === 'email' && !/^[^\s@]+@[^\s@]+\.[^\s@]+$/.test(value)) {
                isValid = false;
                message = 'Please enter a valid email address.';
            } else if (id === 'dob' && !/^\d{2}\/\d{2}\/\d{4}$/.test(value)) {
                isValid = false;
                message = 'Please enter a valid date (MM/DD/YYYY).';
            } else if (id === 'state' && !/^[A-Z]{2}$/.test(value)) {
                isValid = false;
                message = 'Please enter a valid two-letter state code.';
            } else if (id === 'zip' && !/^\d{5}$/.test(value)) {
                isValid = false;
                message = 'Please enter a valid 5-digit ZIP code.';
            } else if (field.type === 'number' && (value < 0 || !Number.isInteger(Number(value)))) {
                isValid = false;
                message = `Please enter a valid ${field.name}.`;
            }

            showError(id, !isValid && !onSubmit, message);
            return isValid;
        }

async function fetchGroups() {
    try {
        document.getElementById('groupCodeSpinner').style.display = 'block';
        document.getElementById('groupCodeTSpinner').style.display = 'block';
        const response = await fetch('https://script.google.com/macros/s/AKfycbxQT1mhqEf3-RAB75o4ct27Jp7so5fs-JIdNaSykBtRD3MVY2OycLPJqO_W5pZKOSLh1Q/exec?sheet=WEB: Groups');
        const data = await response.json();
        // Sort data by group code (row[2]) alphabetically
        data.sort((a, b) => (a[2] || '').localeCompare(b[2] || ''));
        const groupSelects = [document.getElementById('groupCode'), document.getElementById('groupCode_t')];
        groupSelects.forEach(select => {
            select.innerHTML = '<option value="">Select Group';
            data.forEach(row => {
                if (row[2]) {
                    const option = document.createElement('option');
                    option.value = row[2];
                    option.textContent = `${row[2]} - ${row[3]} (${row[4]})`;
                    select.appendChild(option);
                }
            });
        });
    } catch (error) {
        console.error('Error fetching groups:', error);
    } finally {
        document.getElementById('groupCodeSpinner').style.display = 'none';
        document.getElementById('groupCodeTSpinner').style.display = 'none';
    }
}

async function fetchTeams() {
    try {
        document.getElementById('teamCodeSpinner').style.display = 'block';
        document.getElementById('teamCode2Spinner').style.display = 'block';
        const response = await fetch('https://script.google.com/macros/s/AKfycbxQT1mhqEf3-RAB75o4ct27Jp7so5fs-JIdNaSykBtRD3MVY2OycLPJqO_W5pZKOSLh1Q/exec?sheet=WEB: Teams');
        const data = await response.json();
        // Sort data by team code (row[5]) alphabetically
        data.sort((a, b) => (a[5] || '').localeCompare(b[5] || ''));
        const teamSelects = [document.getElementById('teamCode'), document.getElementById('teamCode2')];
        teamSelects.forEach(select => {
            select.innerHTML = '<option value="">Select Team';
            data.forEach(row => {
                if (row[5]) {
                    const option = document.createElement('option');
                    option.value = row[5];
                    option.textContent = `${row[5]} - ${row[6]} (${row[4]})`;
                    select.appendChild(option);
                }
            });
        });
    } catch (error) {
        console.error('Error fetching teams:', error);
    } finally {
        document.getElementById('teamCodeSpinner').style.display = 'none';
        document.getElementById('teamCode2Spinner').style.display = 'none';
    }
}

        async function generateUniqueCode(name, isTeam) {
            const words = name.replace(/[^a-zA-Z\s]/g, '').toUpperCase().split(/\s+/);
            let abbr;
            if (words.length >= 3) {
                abbr = words[0][0] + words[1][0] + words[2][0];
            } else if (words.length === 2) {
                abbr = words[0][0] + words[1].slice(0, 2);
            } else {
                abbr = words[0].slice(0, 3).padEnd(3, 'X');
            }
            let thirdChar = isTeam ? 'C' : abbr[0] === 'C' ? 'X' : abbr[0];
            let code = `26${thirdChar}${abbr.slice(1, 3)}`;
            let attempt = 0;
            const maxAttempts = 100;

            while (attempt < maxAttempts) {
                try {
                    const response = await fetch(`https://script.google.com/macros/s/AKfycbxQT1mhqEf3-RAB75o4ct27Jp7so5fs-JIdNaSykBtRD3MVY2OycLPJqO_W5pZKOSLh1Q/exec?sheet=${isTeam ? 'WEB: Teams' : 'WEB: Groups'}&${isTeam ? 'teamCode' : 'groupCode'}=${code}`);
                    const data = await response.json();
                    if (data.length === 0) {
                        return code;
                    }
                    attempt++;
                    const suffix = String.fromCharCode(65 + (attempt % 26)) + String.fromCharCode(65 + (Math.floor(attempt / 26) % 26));
                    code = `26${thirdChar}${suffix}`;
                } catch (error) {
                    console.error('Error checking code uniqueness:', error);
                    return null;
                }
            }
            throw new Error('Unable to generate a unique code after maximum attempts');
        }

        function getTimestamps() {
            const now = new Date();
            return {
                iso: now.toISOString(),
                mst: now.toLocaleString('en-US', { timeZone: 'America/Denver' })
            };
        }

        async function submitIndividualForm(event) {
            event.preventDefault();
            const submitButton = document.getElementById('individualSubmit');
            submitButton.disabled = true;
            submitButton.classList.add('loading');
            const form = event.target;
            const formData = new FormData(form);
            const age = parseInt(formData.get('age') || 0, 10);
            const isMinor = age < 18;
            const powderhorn = formData.get('powderhorn') === 'yes';
            const coloradoClassic = formData.get('coloradoClassic') === 'yes';

            let isValid = true;
            const fieldsToValidate = [
                'groupCode', 'delegateChaperone', 'firstName', 'lastName', 'gender', 'dob', 'age', 
                'phone', 'email', 'tshirtSize', 'homeAddress', 'city', 'state', 'zip', 
                'emergencyFirstName', 'emergencyLastName', 'emergencyPhone', 'emergencyRelationship', 
                'powderhorn', 'coloradoClassic'
            ];

            if (isMinor) {
                fieldsToValidate.push('guardianFirstName', 'guardianLastName', 'guardianPhone', 'guardianEmail');
            }

            if (powderhorn || coloradoClassic) {
                fieldsToValidate.push('height');
            }

            if (powderhorn) {
                fieldsToValidate.push('weight', 'skierLevel', 'skiSnowboard');
            }

            if (coloradoClassic) {
                fieldsToValidate.push('teamCode', 'jerseyNumber');
                if (formData.get('secondTeam') === 'yes') {
                    fieldsToValidate.push('teamCode2', 'jerseyNumber2');
                }
            }

            fieldsToValidate.forEach(id => {
                const field = document.getElementById(id);
                if (field && !validateField(field, true)) {
                    isValid = false;
                }
            });

            if (!isValid) {
                submitButton.disabled = false;
                submitButton.classList.remove('loading');
                return;
            }

            const timestamps = getTimestamps();
            const data = {
                submitted_at_iso: timestamps.iso,
                submitted_at_mst: timestamps.mst,
                ...Object.fromEntries(formData)
            };

            try {
                const response = await fetch('https://script.google.com/macros/s/AKfycbxQT1mhqEf3-RAB75o4ct27Jp7so5fs-JIdNaSykBtRD3MVY2OycLPJqO_W5pZKOSLh1Q/exec?sheet=WEB: Registration', {
                    method: 'POST',
                    body: JSON.stringify(data)
                });
                const result = await response.json();
                if (result.status === 'success') {
                    const adobeUrl = `https://lbcgjcom.na4.documents.adobe.com/public/esignWidget?wid=CBFCIBAA3AAABLblqZhBfi0abc9_-xF1mOn2z4twVIa8hshbUcOqHt-QZylz2BqKOp-jhTogXXa0dxCtg7FE*#${new URLSearchParams(data).toString()}`;
                    form.reset();
                    document.querySelectorAll('.guardian-field, .event-field, .powderhorn-field, .colorado-classic-field, .second-team-field')
                        .forEach(field => field.classList.add('hidden'));
                    window.location.href = adobeUrl;
                } else {
                    alert('Submission failed: ' + result.message);
                    submitButton.disabled = false;
                    submitButton.classList.remove('loading');
                }
            } catch (error) {
                alert('Submission failed: ' + error.message);
                submitButton.disabled = false;
                submitButton.classList.remove('loading');
            }
        }

        async function submitGroupForm(event) {
            event.preventDefault();
            const submitButton = document.getElementById('groupSubmit');
            submitButton.disabled = true;
            submitButton.classList.add('loading');
            const form = event.target;
            let isValid = true;
            ['groupName_g', 'groupCityState_g', 'leaderFirstName', 'leaderLastName', 'leaderPhone', 'leaderEmail', 'leaderPIN', 'paymentType'].forEach(id => {
                const field = document.getElementById(id);
                if (!validateField(field, true)) isValid = false;
            });

            if (!isValid) {
                submitButton.disabled = false;
                submitButton.classList.remove('loading');
                return;
            }

            const formData = new FormData(form);
            const timestamps = getTimestamps();
            const groupName = formData.get('groupName');
            const groupCode = await generateUniqueCode(groupName, false);
            if (!groupCode) {
                alert('Failed to generate a unique group code.');
                submitButton.disabled = false;
                submitButton.classList.remove('loading');
                return;
            }
            const data = {
                submitted_at_iso: timestamps.iso,
                submitted_at_mst: timestamps.mst,
                groupCode,
                ...Object.fromEntries(formData)
            };

            try {
                const response = await fetch('https://script.google.com/macros/s/AKfycbxQT1mhqEf3-RAB75o4ct27Jp7so5fs-JIdNaSykBtRD3MVY2OycLPJqO_W5pZKOSLh1Q/exec?sheet=WEB: Groups', {
                    method: 'POST',
                    body: JSON.stringify(data)
                });
                const result = await response.json();
                if (result.status === 'success') {
                    alert(`Group registered successfully! Group Code: ${groupCode}`);
                    form.reset();
                    fetchGroups();
                } else {
                    alert('Submission failed: ' + result.message);
                }
            } catch (error) {
                alert('Submission failed: ' + error.message);
            } finally {
                submitButton.disabled = false;
                submitButton.classList.remove('loading');
            }
        }

        async function submitTeamForm(event) {
            event.preventDefault();
            const submitButton = document.getElementById('teamSubmit');
            submitButton.disabled = true;
            submitButton.classList.add('loading');
            const form = event.target;
            let isValid = true;
            ['groupCode_t', 'teamName', 'sport_t', 'mascot', 'coachFirstName', 'coachLastName', 'coachPhone', 'coachEmail', 'coachPIN'].forEach(id => {
                const field = document.getElementById(id);
                if (!validateField(field, true)) isValid = false;
            });

            if (!isValid) {
                submitButton.disabled = false;
                submitButton.classList.remove('loading');
                return;
            }

            const formData = new FormData(form);
            const timestamps = getTimestamps();
            const teamName = formData.get('teamName');
            const teamCode = await generateUniqueCode(teamName, true);
            if (!teamCode) {
                alert('Failed to generate a unique team code.');
                submitButton.disabled = false;
                submitButton.classList.remove('loading');
                return;
            }
            const data = {
                submitted_at_iso: timestamps.iso,
                submitted_at_mst: timestamps.mst,
                teamCode,
                ...Object.fromEntries(formData)
            };

            try {
                const response = await fetch('https://script.google.com/macros/s/AKfycbxQT1mhqEf3-RAB75o4ct27Jp7so5fs-JIdNaSykBtRD3MVY2OycLPJqO_W5pZKOSLh1Q/exec?sheet=WEB: Teams', {
                    method: 'POST',
                    body: JSON.stringify(data)
                });
                const result = await response.json();
                if (result.status === 'success') {
                    alert(`Team registered successfully! Team Code: ${teamCode}`);
                    form.reset();
                    fetchTeams();
                } else {
                    alert('Submission failed: ' + result.message);
                }
            } catch (error) {
                alert('Submission failed: ' + error.message);
            } finally {
                submitButton.disabled = false;
                submitButton.classList.remove('loading');
            }
        }

        document.getElementById('groupCode').addEventListener('change', async (e) => {
            try {
                document.getElementById('groupNameSpinner').style.display = 'block';
                document.getElementById('groupCityStateSpinner').style.display = 'block';
                const response = await fetch(`https://script.google.com/macros/s/AKfycbxQT1mhqEf3-RAB75o4ct27Jp7so5fs-JIdNaSykBtRD3MVY2OycLPJqO_W5pZKOSLh1Q/exec?sheet=WEB: Groups&groupCode=${e.target.value}`);
                const data = await response.json();
                if (data.length) {
                    document.getElementById('groupName').value = data[0][3] || '';
                    document.getElementById('groupCityState').value = data[0][4] || '';
                } else {
                    document.getElementById('groupName').value = '';
                    document.getElementById('groupCityState').value = '';
                }
                showError('groupCode', !e.target.value);
            } catch (error) {
                console.error('Error fetching group details:', error);
            } finally {
                document.getElementById('groupNameSpinner').style.display = 'none';
                document.getElementById('groupCityStateSpinner').style.display = 'none';
            }
        });

        document.getElementById('groupCode_t').addEventListener('change', async (e) => {
            try {
                document.getElementById('groupNameTSpinner').style.display = 'block';
                document.getElementById('groupCityStateTSpinner').style.display = 'block';
                const response = await fetch(`https://script.google.com/macros/s/AKfycbxQT1mhqEf3-RAB75o4ct27Jp7so5fs-JIdNaSykBtRD3MVY2OycLPJqO_W5pZKOSLh1Q/exec?sheet=WEB: Groups&groupCode=${e.target.value}`);
                const data = await response.json();
                if (data.length) {
                    document.getElementById('groupName_t').value = data[0][3] || '';
                    document.getElementById('groupCityState_t').value = data[0][4] || '';
                } else {
                    document.getElementById('groupName_t').value = '';
                    document.getElementById('groupCityState_t').value = '';
                }
                showError('groupCode_t', !e.target.value);
            } catch (error) {
                console.error('Error fetching group details:', error);
            } finally {
                document.getElementById('groupNameTSpinner').style.display = 'none';
                document.getElementById('groupCityStateTSpinner').style.display = 'none';
            }
        });

        document.getElementById('teamCode').addEventListener('change', async (e) => {
            try {
                document.getElementById('teamSpinner').style.display = 'block';
                document.getElementById('sportSpinner').style.display = 'block';
                const response = await fetch(`https://script.google.com/macros/s/AKfycbxQT1mhqEf3-RAB75o4ct27Jp7so5fs-JIdNaSykBtRD3MVY2OycLPJqO_W5pZKOSLh1Q/exec?sheet=WEB: Teams&teamCode=${e.target.value}`);
                const data = await response.json();
                if (data.length) {
                    document.getElementById('team').value = data[0][6] || '';
                    document.getElementById('sport').value = data[0][7] || '';
                } else {
                    document.getElementById('team').value = '';
                    document.getElementById('sport').value = '';
                }
                showError('teamCode', !e.target.value && document.getElementById('coloradoClassic').value === 'yes');
            } catch (error) {
                console.error('Error fetching team details:', error);
            } finally {
                document.getElementById('teamSpinner').style.display = 'none';
                document.getElementById('sportSpinner').style.display = 'none';
            }
        });

        document.getElementById('teamCode2').addEventListener('change', async (e) => {
            try {
                document.getElementById('team2Spinner').style.display = 'block';
                document.getElementById('sport2Spinner').style.display = 'block';
                const response = await fetch(`https://script.google.com/macros/s/AKfycbxQT1mhqEf3-RAB75o4ct27Jp7so5fs-JIdNaSykBtRD3MVY2OycLPJqO_W5pZKOSLh1Q/exec?sheet=WEB: Teams&teamCode=${e.target.value}`);
                const data = await response.json();
                if (data.length) {
                    document.getElementById('team2').value = data[0][6] || '';
                    document.getElementById('sport2').value = data[0][7] || '';
                } else {
                    document.getElementById('team2').value = '';
                    document.getElementById('sport2').value = '';
                }
                showError('teamCode2', !e.target.value && document.getElementById('secondTeam').value === 'yes');
            } catch (error) {
                console.error('Error fetching team details:', error);
            } finally {
                document.getElementById('team2Spinner').style.display = 'none';
                document.getElementById('sport2Spinner').style.display = 'none';
            }
        });

        document.getElementById('age').addEventListener('input', (e) => {
            const isMinor = parseInt(e.target.value || 0, 10) < 18;
            document.querySelectorAll('.guardian-field').forEach(field => {
                field.classList.toggle('hidden', !isMinor);
                const input = field.querySelector('input');
                input.required = isMinor;
                showError(input.id, !input.value && isMinor);
            });
            validateField(e.target);
        });

        document.getElementById('powderhorn').addEventListener('change', (e) => {
            const powderhorn = e.target.value === 'yes';
            const coloradoClassic = document.getElementById('coloradoClassic').value === 'yes';
            const isEvent = powderhorn || coloradoClassic;
            document.querySelectorAll('.powderhorn-field').forEach(field => {
                field.classList.toggle('hidden', !powderhorn);
                const input = field.querySelector('input, select');
                input.required = powderhorn;
                showError(input.id, !input.value && powderhorn);
            });
            document.querySelectorAll('.event-field').forEach(field => {
                field.classList.toggle('hidden', !isEvent);
                const input = field.querySelector('input, select');
                input.required = isEvent;
                showError(input.id, !input.value && isEvent);
            });
            showError('powderhorn', !e.target.value);
        });

        document.getElementById('coloradoClassic').addEventListener('change', (e) => {
            const coloradoClassic = e.target.value === 'yes';
            const powderhorn = document.getElementById('powderhorn').value === 'yes';
            const isEvent = powderhorn || coloradoClassic;
            document.querySelectorAll('.colorado-classic-field').forEach(field => {
                field.classList.toggle('hidden', !coloradoClassic);
                const input = field.querySelector('input, select');
                if (input.id !== 'secondTeam') {
                    input.required = coloradoClassic;
                    showError(input.id, !input.value && coloradoClassic);
                }
            });
            document.querySelectorAll('.event-field').forEach(field => {
                field.classList.toggle('hidden', !isEvent);
                const input = field.querySelector('input, select');
                input.required = isEvent;
                showError(input.id, !input.value && isEvent);
            });
            showError('coloradoClassic', !e.target.value);
        });

        document.getElementById('secondTeam').addEventListener('change', (e) => {
            const isVisible = e.target.value === 'yes';
            document.querySelectorAll('.second-team-field').forEach(field => {
                field.classList.toggle('hidden', !isVisible);
                const input = field.querySelector('input, select');
                input.required = isVisible;
                showError(input.id, !input.value && isVisible);
            });
        });

        document.querySelectorAll('#individualForm input, #individualForm select, #groupForm input, #groupForm select, #teamForm input, #teamForm select').forEach(field => {
            field.addEventListener('input', () => validateField(field));
            field.addEventListener('change', () => validateField(field));
        });

        window.onload = () => {
            fetchGroups();
            fetchTeams();
            document.querySelectorAll('.guardian-field, .event-field, .powderhorn-field, .colorado-classic-field, .second-team-field')
                .forEach(field => field.classList.add('hidden'));
        };
    </script>

